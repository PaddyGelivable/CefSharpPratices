<Window x:Class="CefApplicationTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800" Initialized="Window_Initialized" Closed="Window_Closed">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition MinHeight="550" Height="Auto"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid x:Name="mainGrid"/>
        <Button Grid.Row="1" Click="OpenDev_Click" Width="80" Margin="10, 0, 0, 0" HorizontalAlignment="Left" x:Name="openDev">Open Dev</Button>
    </Grid>
</Window>
